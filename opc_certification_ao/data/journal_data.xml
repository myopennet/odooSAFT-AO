        <function model="account.journal" name="write">
            <value model="account.journal" eval="obj().search([('name', '=', 'Faturas de Cliente')]).ids"/>
            <value eval="{'name': 'Faturas de Cliente',
                          'sequence_id': ref('opc_certification_ao.vendas_sequence'),
                          'refund_sequence_id': ref('opc_certification_ao.retorno_vendas_sequence'),
                          'por_defeito': True,
                          'allow_date': False,
                          'code': 'V'
            }"/>
        </function>

        <function model="account.journal" name="write">
            <value model="account.journal" eval="obj().search([('name', '=', 'Faturas de Fornecedor')]).ids"/>
            <value eval="{'sequence_id': ref('opc_certification_ao.compras_sequence'),
                          'refund_sequence_id': ref('opc_certification_ao.retorno_compras_sequence'),
                          'por_defeito': True,
                          'allow_date': False,
            }"/>
        </function>

        <function model="account.journal" name="write">
            <value model="account.journal" eval="obj().search([('name', '=', 'NumerÃ¡rio')]).ids"/>
            <value eval="{
                          'sequence_id': ref('opc_certification_ao.banco_sequence'),
                          'refund_sequence_id': ref('opc_certification_ao.sequence_pagamentos_fornecedores_caixa'),
                          'por_defeito': True,
